window.onload=function() { if (document.getElementById) {
  var i, rats, rlef, rdow;
  for (var i=0; i<sparkles; i++) {
    if(i%5==0) col1(i);
    else if(i%5==1) col2(i);
    else if(i%5==2) col3(i);
    else if(i%5==3) col4(i);
    else col5(i);
  }
  set_width();
  sparkle();
}
